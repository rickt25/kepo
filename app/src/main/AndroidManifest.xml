<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kepo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kepo">
        <activity android:name=".activities.SearchTodoActivity"/>
        <activity android:name=".activities.UserDetailActivity" />
        <activity android:name=".activities.SearchUserActivity" />
        <activity android:name=".activities.TodoDetailActivity" />
        <activity android:name=".activities.CreateTodoActivity" />
        <activity android:name=".activities.MyTodoActivity" />
        <activity android:name=".activities.ProfileActivity" />
        <activity android:name=".activities.MenuActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.LoginActivity" />
    </application>

</manifest>